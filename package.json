{
  "name": "redux-drive",
  "version": "1.0.0",
  "description":
    "redux-drive is a way to simplify redux boilerplate without a complete rewrite",
  "main": "lib/redux-drive.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "jest ./src -- --coverage",
    "test:watch": "jest ./src",
    "precommit": "pretty-quick --staged",
    "release": "npm run build && npm publish"
  },
  "keywords": [
    "redux",
    "boilerplate",
    "refactor",
    "simple",
    "state-management",
    "cool",
    "awesome"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ohmyjersh/redux-drive.git"
  },
  "author": "Josh Jarmain",
  "bugs": {
    "url": "https://github.com/ohmyjersh/redux-drive/issues"
  },
  "homepage": "https://github.com/ohmyjersh/redux-drive#readme",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "mocha": "^4.0.1",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1",
    "regenerator-runtime": "^0.11.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "jest": {
    "testPathIgnorePatterns": ["<rootDir>/demo/"],
    "coveragePathIgnorePatterns": ["<rootDir>/demo/"]
  }
}
